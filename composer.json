{
    "name": "ludmanp/typicms-transdb",
    "description": "Manage language files and DB",
    "keywords": ["laravel", "localization", "multilingual", "DB ranslation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Leidman",
            "email": "mark@leidman.lv",
            "homepage": "https://ludmanp.github.io"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "~5.1",
        "illuminate/console": "~5.1",
        "illuminate/filesystem": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8 || ^5.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "TypiCMS\\Modules\\TransDB": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TypiCMS\\Modules\\TransDB\\TranslationServiceProvider"
            ]
        }
    }
}
